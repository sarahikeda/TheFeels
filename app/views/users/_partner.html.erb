<div class='partners'>
  <% if @user.partners.any? %>
    <% @user.partners.each do |partner| %>
      <p> Existing Partner Info</p>
      Partner name:<%= partner.name %>
      <br><br>
      Email: <%= partner.email %>
      <br><br>
    <% end %>
  <% else %>
    <%= button_tag "Add Partner", class: "add-partner", data: { disable_with: "Please wait..."} %>

    <div class='new-partner'>
      <%= render 'partners/form' %>
    </div>
  <% end %>
</div>
